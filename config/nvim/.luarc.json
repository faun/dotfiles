{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "workspace.library": [
    "$VIMRUNTIME",
    "./lua",
    "~/.local/share/nvim/lazy",
    "lua",
    "${3rd}/luv/library",
    "${3rd}/busted/library",
    "${3rd}/luassert/library",
    "${3rd}/lazy.nvim/lua",
    "${3rd}/conform.nvim/types"
  ],
  "diagnostics": {
    "disable": [
      "undefined-doc-name"
    ],
    "globals": [
      "vim",
      "LazyVim"
    ]
  },
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "lua/?.lua",
      "lua/?/init.lua"
    ]
  },
  "workspace.checkThirdParty": false
}
